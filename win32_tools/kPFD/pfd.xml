<?xml version="1.0" encoding ="utf-8" ?>
<application xmlns="http://kotuku.aero/schema/kotuku.xsd"
             xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <id>id_units</id>
  <!--<id>id_display_units</id>-->
  <id>id_cancel_alarm</id>
  <id>id_snooze_alarm</id>

  <menu id =" 100"
        root-keys="root"
        alarm-keys="alarm_keys"
        font="neo_9_font"
        background-color="color_black"
        fill-background="false"
        selected-background-color="color_white"
        selected-color="color_magenta"
        border-color="color_yellow"
        text-color="color_green"
        menu-timeout="25">
    <rect left="0" top="0" width="320" height="240"/>
    <menu-rect x="0" y="0"/>
    <menu-start x="0" y="240"/>
    <!-- Menu used when the menu is closed -->
    <keys name="root">
      <key0>
        <popup-action caption="Settings" menu="settings_menu" />
      </key0>
      <key1>
        <event-action caption="Cancel" can-id="id_menu_cancel" value-type="v_int16" value="1"/>
      </key1>
      <decka-up>
        <event-action caption="Next" can-id="id_marquee_next" value-type="v_int16" value="1"/>
      </decka-up>
      <decka-dn>
        <event-action caption="Prev" can-id="id_marquee_prev" value-type="v_int16" value="1"/>
      </decka-dn>
    </keys>
    <!-- Menu used when an alarm is displayed -->
    <keys name="alarm_keys">
      <key0>
        <event-action caption="Close" can-id="id_alarm_close" value-type="v_int16" value="1"/>
      </key0>
      <key1>
        <event-action caption="Park" can-id="id_alarm_park" value-type="v_int16" value="1"/>
      </key1>
    </keys>

    <!-- Menu used when a popup is active -->
    <keys name="popup_keys">
      <key0>
        <event-action caption="OK" can-id="id_menu_ok" value-type="v_int16" value="1"/>
      </key0>
      <key1>
        <event-action caption="Cancel" can-id="id_menu_cancel" value-type="v_int16" value="1"/>
      </key1>
      <decka-up>
        <event-action caption="Up" can-id="id_menu_up" value-type="v_int16" value="1"/>
      </decka-up>
      <decka-dn>
        <event-action caption="Dn" can-id="id_menu_dn" value-type="v_int16" value="1"/>
      </decka-dn>
    </keys>

    <keys name="edit_keys">
      <key0>
        <event-action caption="OK" can-id="id_menu_ok" value-type="v_int16" value="1"/>
      </key0>
      <key1>
        <event-action caption="Cancel" can-id="id_menu_cancel" value-type="v_int16" value="1"/>
      </key1>
      <decka-up>
        <event-action caption="Up" can-id="id_spin_edit" value-type="v_int16" value="10"/>
      </decka-up>
      <decka-dn>
        <event-action caption="Dn" can-id="id_spin_edit" value-type="v_int16" value="-10"/>
      </decka-dn>
      <decka-press-up>
        <event-action caption="Up" can-id="id_spin_edit" value-type="v_int16" value="1"/>
      </decka-press-up>
      <decka-press-dn>
        <event-action caption="Dn" can-id="id_spin_edit" value-type="v_int16" value="-1"/>
      </decka-press-dn>
    </keys>

    <menu name="settings_menu" keys="popup_keys" caption="Settings">
      <popup-action caption="Config" menu="config_menu" />
      <checklist-action caption="ECU Mode" get-index="get_fuel_map" popup="profile_menu"/>
    </menu>

    <menu name="config_menu" keys="popup_keys" caption="Config">
      <checklist-action caption="Units" get-index="get_selected_units" popup="units_menu" />
      <spin-edit-action caption="Brightness"
                        min-value="10"
                        max-value="100"
                        digits="0"
                        get-method="get_brightness"
                        set-method="set_brightness"
                        keys="edit_keys"/>
    </menu>

    <menu name="profile_menu" keys="popup_keys" caption="Lean">
      <event-action caption="Climb" can-id="id_set_fuel_map" value-type="v_uint16" value="0x0002"/>
      <event-action caption="Cruise" can-id="id_set_fuel_map" value-type="v_uint16" value="0x0003"/>
    </menu>

    <menu name="units_menu" keys="popup_keys" caption="Units">
      <event-action caption="SI" can-id="id_display_units" value-type="v_uint16" value="0x8000"/>
      <event-action caption="US" can-id="id_display_units" value-type="v_uint16" value="0x8001"/>
    </menu>
  </menu>

