#include "../muon/cli/neutron.cli"

ion
#submode script
	add <ENUM:can_ids:id> <STRING:name> <STRING:msg_handler>
	del <ENUM:can_ids:id> <STRING:name>
	cat <STRING:name>
	edit <STRING:name>
	create <STRING:name> { <STRING:content> }
	rm <STRING:name>
	ls { <STRING:name> }
	debug { <STRING:name> }
  exec { <STRING:name> }
	exit
#endsubmode
