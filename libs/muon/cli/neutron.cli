neutron
#submode publisher
	ls { <ENUM:can_ids:id> }
	rm <ENUM:can_ids:id>
  publish <ENUM:can_ids:id>
	#submode publish
		rate <UINT16:rate>
		type <ENUM:publish_types:type>
		sample <ENUM:sample_types:sample>
		publish <BOOL:is_published>
		loopback <BOOL:loopback>

		filter <ENUM:filter_types:filter_type>
