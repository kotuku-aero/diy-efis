			length <UINT16:value>
			coeff <UINT16:index> <FLOAT:value>
			gain <FLOAT:value>
			exit
		#endsubmode
		
		alarm <UINT16:alarm_id>
		#submode alarm
			type <ENUM:alarm_types:alarm_type>
			period <UINT16:length>
			max <FLOAT:max_value>
			min <FLOAT:min_value>
			reset <ENUM:can_ids:reset_id>
			exit
		#endsubmode
		exit
	#endsubmode
	exit
#endsubmode

